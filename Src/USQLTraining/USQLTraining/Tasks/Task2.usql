        VALUES
        (
            1,
            new DateTime(2017, 01, 01, 05, 00, 00),
            new DateTime(2017, 01, 01, 06, 00, 00),
            100.00
        )
        ) AS T (id, begin, end, value);

OUTPUT @sample
TO @"d:\Repos\FP-DataSolutions\AzureDataLake\Results\sample.csv"
USING Outputters.Csv(outputHeader : true);